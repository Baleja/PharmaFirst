# LiveKit Cloud (if needed by your Agent SDK)
LIVEKIT_URL=https://<your-livekit-cloud-url>
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# Google/Gemini credentials (service account JSON path)
GOOGLE_APPLICATION_CREDENTIALS=./google-service-account.json

# Alternative: Direct API key (if supported by plugin)
GOOGLE_API_KEY=your_google_api_key

# Local agent port (optional)
AGENT_PORT=4000

# Database (for call session tracking)
DATABASE_URL=postgresql://user:password@localhost:5432/pharmafirst
